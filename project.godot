; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

compatibility/default_parent_skeleton_in_mesh_instance_3d=true

[application]

config/name="LunyScratch_Examples_Godot"
run/main_scene="uid://4tqeylbog06x"
config/features=PackedStringArray("4.6", "C#", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

LunyEngineGodotAdapter="*uid://ss4vx144dk5g"

[debug]

file_logging/enable_file_logging=true
file_logging/max_log_files=2
gdscript/warnings/directory_rules={
"res://addons": 1
}

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=4
window/size/resizable=false

[dotnet]

project/assembly_name="LunyScratch_Examples_Godot"

[editor_plugins]

enabled=PackedStringArray("res://addons/Hidefolders/plugin.cfg", "res://addons/lunyscript/plugin.cfg")

[global_group]

Police=""

[physics]

3d/run_on_separate_thread=true
3d/physics_engine="Jolt Physics"

[rendering]

rendering_device/vsync/swapchain_image_count=2
textures/canvas_textures/default_texture_filter=0
textures/decals/filter=0
textures/light_projectors/filter=0
